				<VerticalSpeed Desc="in m/s">1.5</VerticalSpeed>
				<HorizontalDistance Desc="in m">200</HorizontalDistance>
				<VerticalDistance Desc="in m">40</VerticalDistance>
			</Segment>
			<Segment ID="3">
				<HorizontalAcceleration Desc="in m/s^2">1</HorizontalAcceleration>
				<VerticalAcceleration Desc="in m/s^2">0</VerticalAcceleration>
				<HorizontalSpeed Desc="in m/s">15</HorizontalSpeed>
