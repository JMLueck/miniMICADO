<?xml version="1.0" encoding="utf-8"?>
<ConfigFile Name="miniMICADO_conf.xml">
     <ControlSettings Desc="allgemeine Steuereinstellungen">
        <IODir>..\projekte\</IODir>
		<ConsoleOutputOn>1</ConsoleOutputOn>
        <LogfileOutputOn>1</LogfileOutputOn>
		<LogFile>miniMICADO.log</LogFile>
    </ControlSettings>
    <GeometrySettings Desc="Geometrieeinstellungen">
