	</FuselageSettings>
	<PowertrainSettings>
		<EnergyDensity Desc="in Wh/kg">185</EnergyDensity>
		<PowerDensity Desc="in kW/kg">2.8</PowerDensity> 
	</PowertrainSettings>
	<DesignMissionSettings> 
