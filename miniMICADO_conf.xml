				<VerticalAcceleration Desc="in m/s^2">1</VerticalAcceleration>
				<HorizontalSpeed Desc="in m/s">0</HorizontalSpeed>
				<VerticalSpeed Desc="in m/s">3</VerticalSpeed>
				<HorizontalDistance Desc="in m">0</HorizontalDistance>
